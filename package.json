{
  "name": "ingenieria-prog",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "start": "ng serve",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "deploy": "ng build --configuration=production --base-href=/IngenieriaProg/ && echo '' > dist/IngenieriaProg/browser/.nojekyll && npx angular-cli-ghpages --dir=dist/IngenieriaProg/browser",
    "clean:full": "npm run clean && npm cache clean --force && npm install",
    "build:ssr": "ng build && ng run IngenieriaProg:server:production",
    "serve:ssr": "node dist/IngenieriaProg/server/main.js",
    "prerender": "ng run IngenieriaProg:prerender",
    "serve:ssr:IngenieriaProg": "node dist/IngenieriaProg/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^19.2.18",
    "@angular/common": "^20.0.3",
    "@angular/compiler": "^20.0.3",
    "@angular/forms": "^20.0.3",
    "@angular/material": "^19.2.18",
    "@angular/platform-browser": "^20.0.3",
    "@angular/platform-browser-dynamic": "^20.0.3",
    "@angular/platform-server": "^20.0.3",
    "@angular/router": "^20.0.3",
    "@angular/ssr": "^20.0.2",
    "chart.js": "^3.9.1",
    "express": "^5.1.0",
    "ng2-charts": "^3.1.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.2",
    "@angular/cli": "^20.0.2",
    "@angular/compiler-cli": "^20.0.3",
    "@angular/core": "20.0.3",
    "@angular/ssr": "20.0.2",
    "@tailwindcss/postcss": "^4.1.10",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "angular-cli-ghpages": "^2.0.3",
    "autoprefixer": "^10.4.16",
    "braces": "^3.0.3",
    "fill-range": "^7.1.1",
    "is-number": "^7.0.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "micromatch": "^4.0.8",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.0",
    "to-regex-range": "^5.0.1",
    "typescript": "^5.8.3"
  }
